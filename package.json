{
  "name": "pathfinder",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Fabian Jaramillo",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && yarn server",
    "server": "node server.js",
    "test": "NODE_PATH=./src mocha --recursive ./src/**/*.spec.js",
    "test:watch": "NODE_PATH=./src mocha --watch --recursive ./src/**/*.spec.js",
    "clean": "shx rm -rf dist/*",
    "build": "yarn run clean && cross-env NODE_ENV=production webpack",
    "dev": "webpack-dev-server --progress --colors",
    "lint": "eslint ./src -c .eslintrc",
    "docker-build": "docker build -t property-test .",
    "docker-start": "docker run -p 8080:8080 --name property-test property-test",
    "docker-stop": "docker stop property-test . || true",
    "docker-rm": "docker rm property-test || true",
    "docker": "yarn install && yarn docker-stop && yarn docker-rm && yarn docker-build && yarn docker-start"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.3.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.2.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^4.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "shx": "^0.2.2",
    "express": "^4.15.2"
  }
}
